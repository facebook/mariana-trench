{
  "model_generators": [
    {
      "find": "methods",
      "where": [
        {
          "constraint": "not",
          "inner": {
            "constraint": "parent",
            "inner": {
              "constraint": "name",
              "pattern": "L(com|org)/(mysql|mchange|apache|flywaydb|postgresql)/.*"
            }
          }
        },
        {
          "constraint": "signature",
          "pattern": ".*\\.getPassword:\\(\\).*String;"
        }
      ],
      "model": {
        "sources": [
          {
            "kind": "Password"
          }
        ]
      }
    },
    {
      "find": "methods",
      "where": [
        {
          "constraint": "signature",
          "pattern": "Lcom/facebook/auth/login/AuthServiceHandler;\\.handleOperation:\\(Lcom/facebook/fbservice/service/OperationParams;\\)Lcom/facebook/fbservice/service/OperationResult;"
        }
      ],
      "model": {
        "sources": [
          {
            "kind": "Password",
            "port": "Argument(1)"
          }
        ]
      }
    }
  ]
}
