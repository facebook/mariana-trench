{
  "model_generators": [
    {
      "find": "methods",
      "where": [
        {
          "constraint": "parent",
          "inner": {
            "constraint": "extends",
            "inner": {
              "constraint": "any_of",
              "inners": [
                {
                  "constraint": "name",
                  "pattern": "Lcom/fasterxml/jackson/databind/JsonSerializer;"
                },
                {
                  "constraint": "name",
                  "pattern": "Lcom/fasterxml/jackson/databind/JsonDeserializer;"
                }
              ]
            }
          }
        }
      ],
      "model": {
        "modes": [
          "no-join-virtual-overrides"
        ]
      }
    }
  ]
}
