{
  "model_generators": [
    {
      "find": "methods",
      "where": [
        {
          "constraint": "any_of",
          "inners": [
            {
              "constraint": "signature",
              "pattern": ".*;\\.deserialize:\\(Lcom/facebook/thrift/protocol/TProtocol;\\).*"
            },
            {
              "constraint": "signature",
              "pattern": ".*\\.read:\\(Lcom/facebook/thrift/protocol/TProtocol;\\)V"
            },
            {
              "constraint": "signature",
              "pattern": ".*\\$Serializer;\\.serialize:.*"
            },
            {
              "constraint": "signature",
              "pattern": ".*\\$Deserializer;\\.deserialize:.*"
            },
            {
              "constraint": "signature",
              "pattern": "Lcom/facebook/graphql/model/GraphQLNode$Builder;\\.from:.*"
            },
            {
              "constraint": "signature",
              "pattern": "Lcom/fasterxml/jackson/databind/util/TokenBuffer;\\.serialize:.*"
            },
            {
              "constraint": "signature",
              "pattern": "Lcom/google/android/exoplayer2/upstream/cache/CacheUtil;\\.readAndDiscard:.*"
            },
            {
              "constraint": "signature",
              "pattern": "Lcom/facebook/graphql/model/GraphQLNode.*"
            },
            {
              "constraint": "name",
              "pattern": "mountToGroot"
            },
            {
              "constraint": "name",
              "pattern": "Lcom/facebook/imagepipeline/internal/FbImageDecodeExperimentsProducer;\\.produceResults:.*"
            },
            {
              "constraint": "name",
              "pattern": "Lcom/google/android/exoplayer2/extractor/mp4/FragmentedMp4Extractor;\\.readSample:.*"
            },
            {
              "constraint": "signature",
              "pattern": "Lcom/facebook/common/fragmentfactory/FragmentFactoryMapImpl;.loadNecessaryModule:\\(I\\)V"
            },
            {
              "constraint": "signature",
              "pattern": "Lorg/eclipse/.*;\\.handle:\\(Ljava/lang/String;Lorg/eclipse/jetty/server/Request;Ljavax/servlet/http/HttpServletRequest;Ljavax/servlet/http/HttpServletResponse;\\)V"
            },
            {
              "constraint": "signature",
              "pattern": "Lorg/apache/commons/lang/StringEscapeUtils;\\.escape.*"
            },
            {
              "constraint": "signature",
              "pattern": "Lorg/apache/commons/lang/Entities;\\.doUnescape.*"
            },
            {
              "constraint": "signature",
              "pattern": "Lnet/bytebuddy/agent/builder/AgentBuilder.*"
            },
            {
              "constraint": "signature",
              "pattern": "Lorg/apache/solr/common/util/XML;\\.write.*"
            },
            {
              "constraint": "signature",
              "pattern": "Lorg/elasticsearch/common/lucene/.*"
            },
            {
              "constraint": "signature",
              "pattern": "Lorg/apache/lucene/.*"
            },
            {
              "constraint": "signature",
              "pattern": "Lsun/tools/tree/.*"
            },
            {
              "constraint": "signature",
              "pattern": "Lcom/amazonaws/.*\\.unmarshall:.*"
            },
            {
              "constraint": "signature",
              "pattern": ".*MethodInterceptorGenerator;\\.generateStatic.*"
            }
          ]
        }
      ],
      "model": {
        "modes": [
          "add-via-obscure-feature",
          "skip-analysis",
          "no-join-virtual-overrides",
          "taint-in-taint-out",
          "taint-in-taint-this"
        ]
      }
    }
  ]
}
