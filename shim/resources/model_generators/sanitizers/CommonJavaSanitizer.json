{
  "model_generators": [
    {
      "find": "methods",
      "where": [
        {
          "constraint": "signature",
          "pattern": "Lcom/facebook/dsi/logger/CustomLogger;.*"
        }
      ],
      "model": {
        "modes": [
          "no-join-virtual-overrides"
        ]
      }
    },
    {
      "find": "methods",
      "where": [
        {
          "constraint": "any_of",
          "inners": [
            {
              "constraint": "signature",
              "pattern": ".*ClassReader;\\.readCode.*MethodVisitor.*\\)V"
            },
            {
              "constraint": "signature",
              "pattern": ".*;\\.read0:\\(Lorg/apache/thrift/protocol/TProtocol;\\).*"
            },
            {
              "constraint": "signature",
              "pattern": ".*;\\.write0:\\(Lorg/apache/thrift/protocol/TProtocol;\\)V"
            },
            {
              "constraint": "signature",
              "pattern": ".*;\\.write:\\(Lcom/facebook/thrift/protocol/TProtocol;\\)V"
            }
          ]
        }
      ],
      "model": {
        "modes": [
          "add-via-obscure-feature",
          "skip-analysis",
          "no-join-virtual-overrides",
          "taint-in-taint-out",
          "taint-in-taint-this"
        ]
      }
    },
    {
      "find": "methods",
      "where": [
        {
          "constraint": "name",
          "pattern": "(debug|info|error|fatal|log)"
        },
        {
          "constraint": "parent",
          "inner": {
            "constraint": "extends",
            "inner": {
              "constraint": "name",
              "pattern": "Lorg/slf4j/Logger;"
            }
          }
        }
      ],
      "model": {
        "modes": [
          "skip-analysis"
        ]
      }
    }
  ]
}
