{
  "model_generators": [
    {
      "find": "methods",
      "where": [
        {
          "constraint": "parent",
          "inner": {
            "constraint": "extends",
            "inner": {
              "constraint": "name",
              "pattern": "Ljava/io/InputStream;"
            }
          }
        }
      ],
      "model": {
        "modes": [
          "no-join-virtual-overrides"
        ]
      }
    }
  ]
}
