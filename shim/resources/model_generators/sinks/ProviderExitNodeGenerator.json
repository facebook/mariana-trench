{
  "model_generators": [
    {
      "find": "methods",
      "where": [
        {
          "constraint": "any_of",
          "inners": [
            {
              "constraint": "signature",
              "pattern": ".*;\\.(doC|c)all:\\(Ljava/lang/String;Ljava/lang/String;Landroid/os/Bundle;\\)Landroid/os/Bundle;"
            },
            {
              "constraint": "signature",
              "pattern": ".*;\\.(doO|o)penAssetFile:\\(Landroid/net/Uri;Ljava/lang/String;.*\\)Landroid/content/res/AssetFileDescriptor;"
            },
            {
              "constraint": "signature",
              "pattern": ".*;\\.(doO|o)penFile:\\(Landroid/net/Uri;Ljava/lang/String;.*\\)Landroid/os/ParcelFileDescriptor;"
            },
            {
              "constraint": "signature",
              "pattern": ".*;\\.(doO|o)penPipeHelper:\\(Landroid/net/Uri;Ljava/lang/String;.*\\)Landroid/os/ParcelFileDescriptor;"
            },
            {
              "constraint": "signature",
              "pattern": ".*;\\.(doO|o)penTypedAssetFile:\\(Landroid/net/Uri;Ljava/lang/String;.*\\)Landroid/content/res/AssetFileDescriptor;"
            },
            {
              "constraint": "signature",
              "pattern": ".*;\\.(doQ|q)uery:\\(Landroid/net/Uri;\\[Ljava/lang/String;.*\\)Landroid/database/Cursor;"
            }
          ]
        },
        {
          "constraint": "parent",
          "inner": {
            "constraint": "extends",
            "inner": {
              "constraint": "name",
              "pattern": ".*Provider.*"
            }
          }
        }
      ],
      "model": {
        "sinks": [
          {
            "kind": "ProviderExitNode",
            "port": "Return"
          }
        ]
      }
    }
  ]
}
