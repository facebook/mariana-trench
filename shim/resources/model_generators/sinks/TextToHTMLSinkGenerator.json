{
  "model_generators": [
    {
      "find": "methods",
      "where": [
        {
          "constraint": "signature",
          "pattern": "Landroid/text/Html;\\.fromHtml.*"
        }
      ],
      "model": {
        "sinks": [
          {
            "kind": "HTMLParser",
            "port": "Argument(0)"
          }
        ]
      }
    }
  ]
}
