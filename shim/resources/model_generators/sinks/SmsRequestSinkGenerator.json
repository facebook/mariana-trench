{
  "model_generators": [
    {
      "find": "methods",
      "where": [
        {
          "constraint": "parent",
          "inner": {
            "constraint": "name",
            "pattern": "Landroid/telephony/SmsManager;"
          }
        },
        {
          "constraint": "any_of",
          "inners": [
            {
              "constraint": "name",
              "pattern": "sendDataMessage"
            },
            {
              "constraint": "name",
              "pattern": "sendMultimediaMessage"
            },
            {
              "constraint": "name",
              "pattern": "sendTextMessage"
            },
            {
              "constraint": "name",
              "pattern": "sendMultipartTextMessage"
            },
            {
              "constraint": "name",
              "pattern": "sendTextMessageWithoutPersisting"
            }
          ]
        }
      ],
      "model": {
        "for_all_parameters": [
          {
            "variable": "x",
            "where": [],
            "sinks": [
              {
                "kind": "SmsRequest",
                "port": "Argument(x)"
              }
            ]
          }
        ]
      }
    }
  ]
}
