{
  "model_generators": [
    {
      "find": "methods",
      "where": [
        {
          "constraint": "any_of",
          "inners": [
            {
              "constraint": "parent",
              "inner": {
                "constraint": "name",
                "pattern": "Landroidx/sqlite/db/SupportSQLiteDatabase;"
              }
            },
            {
              "constraint": "parent",
              "inner": {
                "constraint": "name",
                "pattern": "Landroid/database/sqlite/SQLiteQueryBuilder;"
              }
            },
            {
              "constraint": "parent",
              "inner": {
                "constraint": "name",
                "pattern": "Landroid/database/sqlite/SQLiteDatabase;"
              }
            }
          ]
        },
        {
          "constraint": "any_of",
          "inners": [
            {
              "constraint": "name",
              "pattern": "delete.*"
            },
            {
              "constraint": "name",
              "pattern": "execSQL"
            },
            {
              "constraint": "name",
              "pattern": "setTables"
            },
            {
              "constraint": "name",
              "pattern": "rawQuery.*"
            },
            {
              "constraint": "name",
              "pattern": "insert.*"
            },
            {
              "constraint": "name",
              "pattern": "replace.*"
            },
            {
              "constraint": "name",
              "pattern": "query.*"
            },
            {
              "constraint": "name",
              "pattern": "append.*"
            },
            {
              "constraint": "name",
              "pattern": "build.*"
            },
            {
              "constraint": "name",
              "pattern": "update.*"
            }
          ]
        }
      ],
      "model": {
        "modes": [
          "skip-analysis"
        ],
        "for_all_parameters": [
          {
            "variable": "idx",
            "where": [
              {
                "constraint": "not",
                "inner": {
                  "constraint": "name",
                  "pattern": "\\[Ljava/lang/String;"
                }
              },
              {
                "constraint": "not",
                "inner": {
                  "constraint": "name",
                  "pattern": "\\[?Landroid/content/ContentValues;"
                }
              }
            ],
            "sinks": [
              {
                "kind": "SQLQuery",
                "port": "Argument(idx)"
              }
            ]
          }
        ]
      }
    }
  ]
}
