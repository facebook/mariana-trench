{
  "model_generators": [
    {
      "find": "methods",
      "where": [
        {
          "constraint": "signature",
          "pattern": "Lcom/instagram/bloks/hosting/IgBloksScreenBuilder;\\.<init>.*"
        }
      ],
      "model": {
        "sinks": [
          {
            "kind": "BloksScreenCreation",
            "port": "Argument(2)"
          }
        ]
      }
    },
    {
      "find": "methods",
      "where": [
        {
          "constraint": "signature",
          "pattern": "Lcom/whatsapp/wabloks/base/BkScreen;\\.createActivityIntent.*"
        }
      ],
      "model": {
        "for_all_parameters": [
          {
            "variable": "x",
            "where": [
              {
                "constraint": "not",
                "inner": {
                  "constraint": "name",
                  "pattern": "Landroid.*"
                }
              }
            ],
            "sinks": [
              {
                "kind": "BloksScreenCreation",
                "port": "Argument(x)"
              }
            ]
          }
        ]
      }
    },
    {
      "find": "methods",
      "where": [
        {
          "constraint": "signature",
          "pattern": "Lcom/facebook/bloks/facebook/navigation/BloksScreenLauncher\\$Params;\\.(bloks|client|server).*"
        }
      ],
      "model": {
        "sinks": [
          {
            "kind": "BloksScreenCreation",
            "port": "Argument(1)"
          }
        ]
      }
    },
    {
      "find": "methods",
      "where": [
        {
          "constraint": "any_of",
          "inners": [
            {
              "constraint": "signature",
              "pattern": "Lcom/instagram/bloks/hosting/IgBloksScreenBuilder;\\.setAppId.*"
            },
            {
              "constraint": "signature",
              "pattern": "Lcom/instagram/bloks/hosting/IgBloksScreenBuilder;\\.setParams.*"
            },
            {
              "constraint": "signature",
              "pattern": "Lcom/instagram/bloks/hosting/IgBloksScreenBuilder;\\.setNavBarConfigurerFactory.*"
            },
            {
              "constraint": "signature",
              "pattern": "Lcom/instagram/bloks/hosting/IgBloksScreenBuilder;\\.setTitle.*"
            }
          ]
        }
      ],
      "model": {
        "sinks": [
          {
            "kind": "BloksScreenCreation",
            "port": "Argument(1)"
          }
        ]
      }
    }
  ]
}
