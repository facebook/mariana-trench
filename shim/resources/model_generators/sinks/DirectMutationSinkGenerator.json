{
  "model_generators": [
    {
      "find": "methods",
      "where": [
        {
          "constraint": "name",
          "pattern": "<init>"
        },
        {
          "constraint": "parent",
          "inner": {
            "constraint": "extends",
            "inner": {
              "constraint": "name",
              "pattern": "Lcom/instagram/direct/mutation/model/DirectMutation;"
            }
          }
        }
      ],
      "model": {
        "for_all_parameters": [
          {
            "variable": "x",
            "where": [],
            "sinks": [
              {
                "kind": "IGDirectMutation",
                "port": "Argument(x)"
              }
            ]
          }
        ]
      }
    }
  ]
}
