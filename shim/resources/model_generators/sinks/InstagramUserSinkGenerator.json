{
  "model_generators": [
    {
      "find": "methods",
      "where": [
        {
          "constraint": "signature",
          "pattern": "Lcom/instagram/user/model/User;\\.<init>.*"
        }
      ],
      "model": {
        "for_all_parameters": [
          {
            "variable": "x",
            "where": [],
            "sinks": [
              {
                "kind": "ViewerContextCreation",
                "port": "Argument(x)"
              }
            ]
          }
        ]
      }
    }
  ]
}
