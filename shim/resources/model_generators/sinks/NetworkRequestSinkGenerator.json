{
  "model_generators": [
    {
      "find": "methods",
      "where": [
        {
          "constraint": "signature",
          "pattern": "Lorg/apache/http/client/methods/.*;\\.<init>:.*"
        },
        {
          "constraint": "number_parameters",
          "inner": {
            "constraint": "==",
            "value": 2
          }
        }
      ],
      "model": {
        "modes": [
          "skip-analysis"
        ],
        "sinks": [
          {
            "kind": "NetworkRequest",
            "port": "Argument(1)"
          }
        ]
      }
    },
    {
      "find": "methods",
      "where": [
        {
          "constraint": "signature",
          "pattern": "(Lcom/instagram/api/base/IgApi$Builder;.*)|(Lcom/instagram/common/api/facebook/FacebookGraphApi$Builder;.*)"
        },
        {
          "constraint": "name",
          "pattern": "(add)|(set)"
        }
      ],
      "model": {
        "modes": [
          "skip-analysis"
        ],
        "for_all_parameters": [
          {
            "variable": "x",
            "where": [],
            "sinks": [
              {
                "kind": "NetworkRequest",
                "port": "Argument(x)"
              }
            ]
          }
        ]
      }
    },
    {
