{
  "model_generators": [
    {
      "find": "methods",
      "where": [
        {
          "constraint": "is_static",
          "value": true
        },
        {
          "constraint": "is_constructor",
          "value": false
        },
        {
          "constraint": "visibility",
          "is": "public"
        },
        {
          "constraint": "any_of",
          "inners": [
            {
              "constraint": "signature",
              "pattern": "Lcom/facebook/common/uri/(UriIntentBuilder|UriIntentMapper);.+:\\(.+\\)Landroid/content/Intent;"
            },
            {
              "constraint": "signature",
              "pattern": "Lcom/facebook/intent/thirdparty/NativeThirdPartyUriHelper;.+:\\(.+\\)Landroid/content/Intent;"
            }
          ]
        }
      ],
      "model": {
        "modes": [
          "no-join-virtual-overrides"
        ],
        "propagation": [
          {
            "input": "Argument(1)",
            "output": "Return",
            "features": [
              "via-uri-intent-builder"
            ]
          }
        ]
      }
    },
    {
      "find": "methods",
      "where": [
        {
          "constraint": "is_static",
          "value": false
        },
        {
          "constraint": "is_constructor",
          "value": false
        },
        {
          "constraint": "visibility",
          "is": "public"
        },
        {
          "constraint": "any_of",
          "inners": [
            {
              "constraint": "signature",
              "pattern": "Lcom/facebook/common/uri/(UriIntentBuilder|UriIntentMapper);.+:\\(.+\\)Landroid/content/Intent;"
            },
            {
              "constraint": "signature",
              "pattern": "Lcom/facebook/intent/thirdparty/NativeThirdPartyUriHelper;.+:\\(.+\\)Landroid/content/Intent;"
            }
          ]
        }
      ],
      "model": {
        "modes": [
          "no-join-virtual-overrides"
        ],
        "propagation": [
          {
            "input": "Argument(2)",
            "output": "Return",
            "features": [
              "via-uri-intent-builder"
            ]
          }
        ]
      }
    }
  ]
}
