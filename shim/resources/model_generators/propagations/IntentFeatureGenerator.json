{
  "model_generators": [
    {
      "find": "methods",
      "where": [
        {
          "constraint": "signature",
          "pattern": "Landroid/content/Intent;\\.setData.*"
        }
      ],
      "model": {
        "propagation": [
          {
            "input": "Argument(1)",
            "output": "Argument(0)",
            "features": [
              "via-intent-data"
            ]
          }
        ]
      }
    },
    {
      "find": "methods",
      "where": [
        {
          "constraint": "signature",
          "pattern": "Landroid/content/Intent;\\.setSelector.*"
        }
      ],
      "model": {
        "propagation": [
          {
            "input": "Argument(1)",
            "output": "Argument(0)",
            "features": [
              "via-intent-selector"
            ]
          }
        ]
      }
    },
    {
      "find": "methods",
      "where": [
        {
          "constraint": "signature",
          "pattern": "Landroid/content/Intent;\\.<init>:.*android/net/Uri.*"
        }
      ],
      "model": {
        "for_all_parameters": [
          {
            "variable": "x",
            "where": [
              {
                "constraint": "name",
                "pattern": "Landroid/net/Uri;"
              }
            ],
            "propagation": [
              {
                "input": "Argument(x)",
                "output": "Return",
                "features": [
                  "via-intent-data"
                ]
              }
            ]
          }
        ]
      }
    },
    {
      "find": "methods",
      "where": [
        {
          "constraint": "signature",
          "pattern": "Landroid/content/Intent;\\.putExtra.*"
        }
      ],
      "model": {
        "for_all_parameters": [
          {
            "variable": "x",
            "where": [],
            "propagation": [
              {
                "input": "Argument(x)",
                "output": "Argument(0)",
                "features": [
                  "via-intent-extra"
                ]
              }
            ]
          }
        ]
      }
    },
    {
      "find": "methods",
      "where": [
        {
          "constraint": "parent",
          "inner": {
            "constraint": "name",
            "pattern": "Landroid/content/Intent;"
          }
        },
        {
          "constraint": "any_of",
          "inners": [
            {
              "constraint": "name",
              "pattern": "setClass.*"
            },
            {
              "constraint": "name",
              "pattern": "setComponent"
            },
            {
              "constraint": "name",
              "pattern": "setPackage"
            }
          ]
        }
      ],
      "model": {
        "add_features_to_arguments": [
          {
            "features": [
              "via-explicit-intent"
            ],
            "port": "Argument(0)"
          }
        ]
      }
    },
    {
      "find": "methods",
      "where": [
        {
          "constraint": "parent",
          "inner": {
            "constraint": "name",
            "pattern": "Landroid/content/Intent;"
          }
        },
        {
          "constraint": "name",
          "pattern": "setData.*"
        }
      ],
      "model": {
        "add_features_to_arguments": [
          {
            "features": [
              "has-intent-data"
            ],
            "port": "Argument(0)"
          }
        ]
      }
    },
    {
      "find": "methods",
      "where": [
        {
          "constraint": "parent",
          "inner": {
            "constraint": "name",
            "pattern": "Landroid/content/Intent;"
          }
        },
        {
          "constraint": "name",
          "pattern": "addCategory"
        }
      ],
      "model": {
        "add_features_to_arguments": [
          {
            "features": [
              "has-intent-category"
            ],
            "port": "Argument(0)"
          }
        ]
      }
    },
    {
      "find": "methods",
      "where": [
        {
          "constraint": "parent",
          "inner": {
            "constraint": "name",
            "pattern": "Landroid/content/Intent;"
          }
        },
        {
          "constraint": "name",
          "pattern": "setAction"
        }
      ],
      "model": {
        "add_features_to_arguments": [
          {
            "features": [
              "has-intent-action"
            ],
            "port": "Argument(0)"
          }
        ]
      }
    },
    {
      "find": "methods",
      "where": [
        {
          "constraint": "parent",
          "inner": {
            "constraint": "name",
            "pattern": "Landroid/content/Intent;"
          }
        },
        {
          "constraint": "name",
          "pattern": "\\<init\\>"
        },
        {
          "constraint": "parameter",
          "idx": 1,
          "inner": {
            "constraint": "name",
            "pattern": "Ljava/lang/String;"
          }
        }
      ],
      "model": {
        "add_features_to_arguments": [
          {
            "features": [
              "has-intent-action"
            ],
            "port": "Argument(0)"
          }
        ]
      }
    }
  ]
}
