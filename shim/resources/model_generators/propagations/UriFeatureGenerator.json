{
  "model_generators": [
    {
      "find": "methods",
      "where": [
        {
          "constraint": "signature",
          "pattern": "Landroid/net/Uri;\\.getQuery.*"
        }
      ],
      "model": {
        "propagation": [
          {
            "input": "Argument(0)",
            "output": "Return",
            "features": [
              "via-uri-query-parameter"
            ]
          }
        ]
      }
    }
  ]
}
